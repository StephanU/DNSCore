    <mets:structMap TYPE="LOGICAL">
        <mets:div TYPE="volume" ADMID="amdSec">
            <mets:div TYPE="preface" ID="div_idm1094368">
                <mets:div TYPE="chapter" LABEL="" ID="div_idm1045928"/>
            </mets:div>
            <mets:div TYPE="text" ID="div_idp102544">
                <mets:div TYPE="monograph" ID="div_idm1092496" DMDID="dmdSec_idm1092496"
                    LABEL="De pollutione nocturna">
                    <mets:div TYPE="chapter"
                        LABEL="Incipit Tractatus venerabilis Magistri Johannis Gerson cancellarii parisiensis tractans de pollucione nocturna. an impediat celebrantem vel non"
                        ID="div_idm1041944">
                        <mets:div TYPE="chapter"
                            LABEL="Ubitantum est apud me frequenter et diu presertim post susceptum sacerdotium. si quis nocturno pollutus sompnio a celebrando missam cessare deberet"
                            ID="div_idm1148696"/>
                    </mets:div>
                    <mets:div TYPE="chapter" LABEL="Sequitur Quinta consideracio"
                        ID="div_idm1130872">
                        <mets:div TYPE="chapter"
                            LABEL="Nulla pollutio inchoata et perfecta in sompno peccatum est mortale"
                            ID="div_idm1042600"/>
                    </mets:div>
                    <mets:div TYPE="chapter" LABEL="Nona consideratio" ID="div_idm1403896">
                        <mets:div TYPE="chapter"
                            LABEL="Precipue ex divino munere et non ex nostra industria seu preparatione reddimur digni misteriorum participacione sacramentorum"
                            ID="div_idm1416832"/>
                    </mets:div>
                </mets:div>
                <mets:div TYPE="monograph" ID="div_idm1415288" DMDID="dmdSec_idm1415288"
                    LABEL="De cognitione castitatis et de pollutionibus diurnis Forma absolutionis sacramentalis">
                    <mets:div TYPE="chapter"
                        LABEL="Incipit Tractatulus venerabilis Magistri Johannis Gerson Cancellarii parisiensis. de Cognicione castitatis et pollucionibus diurnis"
                        ID="div_idm1095880">
                        <mets:div TYPE="chapter" LABEL="Consideratio prima" ID="div_idp3979568">
                            <mets:div TYPE="chapter"
                                LABEL="Presupponendum est igitur in primis. quod a corpore humano defluit multiplex humor permeatus membrorum pudendorum que generationi deputata sunt. unde est vero seminalis. alius vero urinalis. alius quodammodo medius qui potest appellari salivalis vel sputalis"
                                ID="div_idm1130192"/>
                        </mets:div>
                    </mets:div>
                    <mets:div TYPE="chapter" LABEL="Consideratio sexta" ID="div_idm1382016">
                        <mets:div TYPE="chapter"
                            LABEL="Quo circa dicamus Sexto consequenter quod distingui solet in voluntate duplex consensus. unus completus deliberativus et indirectus"
                            ID="div_idm1285080"/>
                    </mets:div>
                    <mets:div TYPE="chapter" LABEL="Propositio octava" ID="div_idm1005128">
                        <mets:div TYPE="chapter"
                            LABEL="Difficile est vel impossibile absque divina illuminatione cognoscere certitudinaliter radicem et causa pollutionis diurne si videlicet veniat ex consensu vero vel interpretativo aut consensu directo vel indirecto aut a consensu superioris rationis vel inferioris provenit"
                            ID="div_idm1040608"/>
                    </mets:div>
                    <mets:div TYPE="chapter"
                        LABEL="Incipit forma absolucionis sacramentalis eiusdem Magistri Joh(annis) Gerson"
                        ID="div_idm995368">
                        <mets:div TYPE="chapter"
                            LABEL="Gratia tibi et pax frater carissime petis primo si apud aliquem  doctorem reperiatur forma absolutionis  sacramentalis"
                            ID="div_idm1417832"/>
                    </mets:div>
                </mets:div>
                <mets:div TYPE="monograph" ID="div_idm990592" DMDID="dmdSec_idm990592"
                    LABEL="De simonia. Add: De probatione spirituum De eruditione confessorum De remediis contra recidivum peccandi">
                    <mets:div TYPE="chapter"
                        LABEL="Incipit Tractatus de symonia Magistri Johannis Gerson Cancellarii parisiensis"
                        ID="div_idm1103376">
                        <mets:div TYPE="chapter"
                            LABEL="Simonaica labes contra ius divinum vel de simonaica spspem suspectum est in conferente beneficium ecclesiarum"
                            ID="div_idm1043240"/>
                    </mets:div>
                    <mets:div TYPE="chapter"
                        LABEL="Denique pro responsione ad septimam racionem consideratio septuplex notabiliter satis confertur ad premissa"
                        ID="div_idm1059864">
                        <mets:div TYPE="chapter" LABEL="Prima consideratio" ID="div_idm485504">
                            <mets:div TYPE="chapter"
                                LABEL="Simonaica labes sicut de per se non est heresis. Sic non aufert a papa neccessario suam potestate vel ecclesiastice potestatis plenitudinem. Ymmo nec execucionem"
                                ID="div_idm485064"/>
                        </mets:div>
                    </mets:div>
                    <mets:div TYPE="chapter"
                        LABEL="Incipit tractatus de probatione spirituum Magistri Johannis Gerson. Cancellarii parisiensis"
                        ID="div_idp3980432">
                        <mets:div TYPE="chapter"
                            LABEL="Probate spiritus si ex deo sunt iubet discipulus ille quem diligebat ihesus  Neque enim nesciebat illam coapostoli fui sentenciam"
                            ID="div_idp3980968"/>
                    </mets:div>
                    <mets:div TYPE="chapter"
                        LABEL="Incipit tractatus de eruditione confessorum Magistri Johannis Gerson . Cancellarii parisiensis"
                        ID="div_idm15408">
                        <mets:div TYPE="chapter"
                            LABEL="Et si virtus quam assuefactio gignit certius quam ars  operetur. nonnichil tamen prodest artis tradicio apud eos qui nondum perfecti sunt in virtute. hoc in arte quomlibet perspiciuum est"
                            ID="div_idm14864"/>
                    </mets:div>
                    <mets:div TYPE="chapter"
                        LABEL="Incipit  tractatus Magistri Johannis Gerson . de remediis contrarecidivum peccandi a confessoribus dandis"
                        ID="div_idm967472">
                        <mets:div TYPE="chapter"
                            LABEL="Habet hoc primum ars quelibet ut faciliori conpendio ad finem ducat optatum ammirari preterea  nemo debet si in animarum regimine. quod est ars arcium. experti in ea faciliora quidem sciunt et dant remedia  que morbos vitiorum presertim in foro conscientie ubi maxime locum habet illa christi formidabilis sententia"
                            ID="div_idm966920"/>
                    </mets:div>
                </mets:div>
                <mets:div TYPE="monograph" ID="div_idm986792" DMDID="dmdSec_idm986792"
                    LABEL="De remediis contra pusillanimitatem contra deceptorias inimici consolationes eiusque tentationes">
                    <mets:div TYPE="chapter" LABEL="" ID="div_idm480968"/>
                    <mets:div TYPE="chapter"
                        LABEL="Incipit Tractatulus de Remediis contra pusillamitatem Scrupulositate . contra deceptorias inimici consolaciones. et subtiles eius temptaciones. Per Illuminatissimum Magistrum Johannem Gerson. doctorem sacre Theologie. Cancellarium Parisiensis. Edittus"
                        ID="div_idm1372408">
                        <mets:div TYPE="chapter"
                            LABEL="Qui pusillaminis est pavidus. cavere sibi debet a nimio timore. Si ei cor suum et naturalia sequitur. ad desperacionem vel nimiam mentis deiectione perveniet"
                            ID="div_idm1371712"/>
                    </mets:div>
                    <mets:div TYPE="chapter"
                        LABEL="Explicit tractatulus de remediis contra pusillanimitatem. scrupulositatem. et deceptorias inimici consolationes. et subtiles eius temptationes. venerabilis Magistri Joh(annis) Gerson. Cancellarii parisiensis "
                        ID="div_idm94720"/>
                </mets:div>
            </mets:div>
            <mets:div TYPE="section" ID="div_idm93160">
                <mets:div TYPE="chapter" LABEL="" ID="div_idm92904"/>
            </mets:div>
        </mets:div>
